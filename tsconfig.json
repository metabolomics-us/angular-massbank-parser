{
  "compilerOptions": {
    "module": "commonjs",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": ["node"],
    "declaration": true,
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": ["es5", "dom", "es2015"],
    "target": "es6",
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "removeComments": false,
    "outDir": "./lib"
  },
  "include": [
    "./service.ts"
  ]
}
